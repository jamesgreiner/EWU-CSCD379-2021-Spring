@model UserViewModel

<section class="m-2">
    <form asp-controller="Users" asp-action="Create" method="post">
        @await Html.PartialAsync("_CreateOrEdit")
        
        <button class="btn mr-2 mt-2">Create</button>
        <a class="btn inline-block bg-red-500 mt-2" asp-controller="Users" asp-action="Index">Cancel</a>
    </form>
</section>